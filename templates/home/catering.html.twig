{% extends 'base.html.twig' %}

{% block body %}

    <div id="logo">
        <h1>{{ Title }}</h1>
    </div>

    <hr>

    <base href="{{ asset('images/c9.jpg') }}">
<div id="carousel">
  <figure id="spinner">
    <img src="{{ asset('images/c1.webp') }}" alt>
    <img src="{{ asset('images/c2.webp') }}" alt>
    <img src="{{ asset('images/c3.jpg') }}" alt>
    <img src="{{ asset('images/c4.jpeg') }}" alt>
    <img src="{{ asset('images/c5.jpeg') }}" alt>
    <img src="{{ asset('images/c6.jpg') }}" alt>
    <img src="{{ asset('images/c7.jpg') }}" alt>
    <img src="{{ asset('images/c8.jpg') }}" alt>
  </figure>
</div>
<span style="float:left" class="ss-icon" onclick="galleryspin('-')">&lt;</span>
<span style="float:right" class="ss-icon" onclick="galleryspin('')">&gt;</span>

{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/script.js') }}"></script>
{% endblock %}